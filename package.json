{
  "name": "rinobot-app",
  "productName": "rinobot",
  "version": "0.0.5",
  "license": "MIT",
  "repository": "rinocloud/rinobot",
  "description": "Automate data tasks",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "build": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./index.dev",
    "start": "cross-env NODE_ENV=production electron ./index",
    "check-dev-engines": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "postinstall": "npm run check-dev-engines"
  },
  "devDependencies": {
    "aws-sdk": "^2.4.7",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "concurrently": "^2.1.0",
    "cross-env": "^1.0.8",
    "del": "^2.2.0",
    "electron-packager": "^7.0.2",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-plugin-import": "^1.10.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "fbjs-scripts": "^0.7.1",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3"
  },
  "dependencies": {
    "chokidar": "^1.6.0",
    "electron-prebuilt": "^1.2.7",
    "source-map-support": "^0.4.2",
    "webpack": "^1.13.1"
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "mocha": true,
      "node": true
    },
    "rules": {
      "consistent-return": 0,
      "comma-dangle": 0,
      "no-use-before-define": 0,
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "electron"
          ]
        }
      ],
      "react/jsx-no-bind": 0,
      "react/prefer-stateless-function": 0,
      "semi": 0,
      "func-names": 0,
      "no-underscore-dangle": 0,
      "no-console": 0
    },
    "plugins": [
      "import",
      "react"
    ]
  }
}

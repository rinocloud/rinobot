{
  "name": "rinobot-app-renderer",
  "productName": "rinobot",
  "version": "0.0.5",
  "license": "MIT",
  "repository": "rinocloud/rinobot",
  "description": "Automate data tasks",
  "scripts": {
    "hot-server": "node -r babel-register server.js",
    "build": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors"
  },
  "dependencies": {
    "js-yaml": "^3.6.1",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.14.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-actions": "^0.9.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.3",
    "superagent": "^1.8.3",
    "tar": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.5.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-plugin-import": "^1.10.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "add-module-exports"
    ],
    "env": {
      "production": {
        "presets": [
          "react-optimize"
        ]
      },
      "development": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "mocha": true,
      "node": true
    },
    "rules": {
      "consistent-return": 0,
      "comma-dangle": 0,
      "no-use-before-define": 0,
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "electron"
          ]
        }
      ],
      "react/jsx-no-bind": 0,
      "react/prefer-stateless-function": 0,
      "semi": 0,
      "func-names": 0,
      "no-underscore-dangle": 0,
      "no-console": 0
    },
    "plugins": [
      "import",
      "react"
    ]
  }
}
